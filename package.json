{
  "name": "dnd-spellbook",
  "version": "1.0.0",
  "source": "src/index.html",
  "scripts": {
    "fetch-spells": "npx tsc && node dist/src/fetch-spells/index.js",
    "link-spells": "npx tsc && node dist/src/link-spells/index.js",
    "start": "parcel",
    "build": "parcel build",
    "publish": "echo '\nBUILDING...\n' && npm run build && echo '\nPUBLISHING...\n' && git add -Af docs && git commit --allow-empty -m 'Publish' && git push && echo '\nPUBLISHED.'"
  },
  "author": "Sam Fritton",
  "license": "MIT",
  "devDependencies": {
    "@parcel/transformer-inline-string": "^2.9.3",
    "@tanstack/eslint-plugin-query": "^5.28.6",
    "@types/debounce": "^1.2.1",
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "parcel": "^2.9.3",
    "posthtml-doctype": "^1.1.1",
    "posthtml-include": "^1.7.4",
    "process": "^0.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.6"
  },
  "browserslist": "> 0.15% in US and not dead and supports es6-module, safari > 11, last 12 firefox versions, last 12 chrome versions, last 12 edge versions",
  "targets": {
    "default": {
      "distDir": "./docs",
      "publicUrl": "./",
      "optimize": false,
      "outputFormat": "esmodule"
    }
  },
  "@parcel/transformer-css": {
    "drafts": {
      "nesting": true
    }
  },
  "dependencies": {
    "@tanstack/react-query": "^5.28.9",
    "cheerio": "^1.0.0-rc.12",
    "debounce": "^1.2.1",
    "html2canvas": "^1.4.1"
  }
}